{
  "name": "@arundo/react-shell",
  "version": "1.2.1",
  "description": "Core functionality for Arundo React apps",
  "engines": {
    "node": ">=11.0.0"
  },
  "main": "index.js",
  "target": "esnext",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "dev",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prebuild": "rm -rf dist",
    "build": "babel src --ignore '**/*.spec.js' --out-dir dist -d",
    "prepublish": "cp ../../package.json package.json",
    "postpublish": "cp package.json ../../package.json && cd ../.."
  },
  "author": "Kevin R. Whitley <kevin.whitley@arundo.com>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/arundo/react-shell.git"
  },
  "bugs": {
    "url": "https://github.com/arundo/react-shell/issues",
    "email": "support@arundo.com"
  },
  "devDependencies": {
    "@arundo/spa-builder": "^1.7.1",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-datetime": "^1.5.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.14.1",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "@reach/router": "^1.2.1",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-icons": "^3.4.0"
  },
  "peerDependencies": {
    "@reach/router": "^1.2.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "title": "Arundo App",
  "heroku-run-build-script": true
}
