{
  "name": "@arundo/react-shell",
  "version": "1.4.1",
  "description": "Core functionality for Arundo React apps",
  "main": "core.js",
  "target": "esnext",
  "title": "React Shell",
  "scripts": {
    "start": "node dist/server.js",
    "prebuild": "rm -rf build && mkdir build",
    "copy:structure": "cp -R src/client/* build",
    "remove:jsx": "find build -name '*.jsx' -delete",
    "transpile": "babel src/client --ignore '**/*.spec.js' --out-dir build -d",
    "build": "yarn copy:structure && yarn transpile && yarn remove:jsx",
    "release:major": "yarn build && release --major --src=build --push",
    "release:minor": "yarn build && release --minor --src=build --push",
    "release:patch": "yarn build && release --patch --src=build --push",
    "verify": "yarn build && release --patch --src=build --push --test --nocleanup",
    "dev": "dev"
  },
  "author": "Kevin R. Whitley <kevin.whitley@arundo.com>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/arundo/react-shell.git"
  },
  "bugs": {
    "url": "https://github.com/arundo/react-shell/issues",
    "email": "support@arundo.com"
  },
  "dependencies": {
    "auth0-js": "^9.10.1",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@arundo/express-starter": "^1.2.9",
    "@arundo/spa-builder": "^1.9.8",
    "@arundo/react-auth": "^2.0.0",
    "@reach/router": "^1.2.1",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@kwhitley/yarn-release": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "bluebird": "^3.5.3",
    "chai": "^4.2.0",
    "chai-datetime": "^1.5.0",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.3",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^6.0.2",
    "np": "^4.0.2",
    "nyc": "^13.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-icons": "^3.5.0",
    "react-virtualized": "^9.21.0"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "@arundo/react-auth": "^2.0.0"
  }
}
